{
    "extends": [
        "next",
        "next/core-web-vitals"
    ],
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": ["warn", { "allow": ["error"] }],
        "@typescript-eslint/no-unused-vars": ["error", { 
            "argsIgnorePattern": "^_",
            "varsIgnorePattern": "^_"
        }],
        "@typescript-eslint/no-explicit-any": "off",
        "@typescript-eslint/ban-types": "off"
    }
}
